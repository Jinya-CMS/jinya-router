{
  "name": "jinya/router",
  "description": "A simple attribute-based routing library",
  "license": "MIT",
  "authors": [
    {
      "name": "Imanuel Ulbricht",
      "email": "imanuel.ulbricht@jinya.de"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-fileinfo": "*",
    "psr/http-server-middleware": "^1.0.2",
    "nikic/fast-route": "^v1.3.0",
    "laminas/laminas-httphandlerrunner": "^2.7.0",
    "laminas/laminas-stratigility": "^3.10.0",
    "laminas/laminas-diactoros": "^3.0.0",
    "jetbrains/phpstorm-attributes": "^1.0",
    "nyholm/psr7": "^1.8.0",
    "nyholm/psr7-server": "^1.0.2"
  },
  "require-dev": {
    "ext-xdebug": "*",
    "roave/security-advisories": "dev-latest",
    "phpstan/phpstan": "1.10.32",
    "phpunit/phpunit": "10.3.2",
    "laravel/pint": "1.13.1"
  },
  "autoload": {
    "psr-4": {
      "Jinya\\Router\\": "src/"
    },
    "files": [
      "src/handle.php",
      "src/build.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Jinya\\Router\\Tests\\": "tests/"
    },
    "files": [
      "tests/functions/functionMiddlewareTestFunctions.php"
    ]
  }
}
